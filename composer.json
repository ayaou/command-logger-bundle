{
  "name": "ayaou/command-logger-bundle",
  "type": "symfony-bundle",
  "description": "This bundle logs all commands executed with console, along with their execution time and output.",
  "keywords": ["symfony", "command", "logger", "log"],
  "homepage": "https://github.com/ayaou/command-logger-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Mohamed AYAOU",
      "email": "ayaou.mohammed@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "symfony/console": "^6.4",
    "symfony/event-dispatcher": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/uid": "^6.4",
    "symfony/yaml": "^6.4",
    "doctrine/doctrine-bundle": "^2.7",
    "doctrine/orm": "^2.14"
  },
  "require-dev": {
    "phpunit/phpunit": "^10",
    "rector/rector": "*",
    "friendsofphp/php-cs-fixer": "*",
    "phpstan/phpstan-symfony": "*",
    "roave/security-advisories": "dev-latest",
    "symfony/phpunit-bridge": "^6.4",
    "symfony/framework-bundle": "^6.4"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Ayaou\\CommandLoggerBundle\\": "src/",
      "Ayaou\\CommandLoggerBundle\\Tests\\": "tests/"
    }
  }
}